{
	"name": "@e18e/mcp",
	"mcpName": "dev.e18e/e18e",
	"version": "0.0.5",
	"publishConfig": {
		"access": "public"
	},
	"description": "The official e18e MCP server for advising agents on modern and performant best practices",
	"type": "module",
	"bin": {
		"e18e-mcp": "dist/index.js"
	},
	"homepage": "https://github.com/e18e/mcp#readme",
	"bugs": {
		"url": "https://github.com/e18e/mcp/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/e18e/mcp.git",
		"path": "packages/stdio"
	},
	"files": [
		"./dist"
	],
	"scripts": {
		"build": "tsgo && pnpm generate:docs && pnpm copy-assets && publint",
		"copy-assets": "node ./scripts/copy-assets.ts",
		"generate:docs:dev": "node ./scripts/fetch-docs.ts dev",
		"generate:docs": "node ./scripts/fetch-docs.ts",
		"inspect": "pnpm build && mcp-inspector --transport stdio node ./dist/index.js",
		"update:version": "node scripts/update-version.ts",
		"test": "vitest"
	},
	"keywords": [
		"tmcp",
		"mcp",
		"server",
		"e18e"
	],
	"dependencies": {
		"@tmcp/adapter-valibot": "catalog:mcp",
		"@tmcp/transport-stdio": "catalog:mcp",
		"es-module-lexer": "catalog:tooling",
		"module-replacements": "^2.10.1",
		"tmcp": "catalog:mcp",
		"valibot": "catalog:tooling",
		"vitest": "catalog:tooling"
	},
	"devDependencies": {
		"@modelcontextprotocol/inspector": "catalog:mcp",
		"@types/node": "catalog:tooling",
		"@typescript/native-preview": "catalog:tooling",
		"publint": "catalog:tooling"
	}
}
